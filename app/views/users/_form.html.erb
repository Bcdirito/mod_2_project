<% if flash[:message] %>
  <strong><%= flash[:message] %></strong>
<% end %>
<br>

<%= form_for user do |f| %>

  <ul><%= f.label :first_name %>:
  <%= f.text_field :first_name %></ul>

  <ul><%= f.label :last_name %>:
  <%= f.text_field :last_name %></ul>

  <ul><%= f.label :user_name %>:
  <%= f.text_field :user_name %></ul>

  <ul><%= f.label :password %>:
  <%= f.password_field :password %></ul>

  <ul><%= f.label :password_confirmation %>:
  <%= f.password_field :password_confirmation %></ul>
  
  <ul><%= f.submit "Submit" %></ul>

<% end %>
