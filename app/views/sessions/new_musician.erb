<h1>Login Musician</h1>

<% if !flash[:message].nil? %>
  <strong><%= flash[:message] %></strong>
<% end %>
<%= form_with url: "/login_musician" do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username, :autocomplete => :off %>

  <%= f.label :password %>
  <%= f.password_field :password, :autocomplete => :off %>
  <%= f.submit "Login" %>
<% end %>
