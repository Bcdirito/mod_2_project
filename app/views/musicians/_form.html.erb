<%= form_for musician do |f| %>
<%= f.label :name %>
<%= f. text_field :name %>

<%= f.label :genre %>
<%= f.collection_check_boxes :genre, @genres, :id, :name%>

<%= f.label :band_members%>
<%= f. text_field :band_members %>

<%= f.label :bio %>
<%= f. text_area :bio %>

<%= f.label :image %>
<%= f. text_field :image %>

<%= f.submit %>
<% end %>
